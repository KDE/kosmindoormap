{
    "id": "org.kde.kosmindoormap",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.6",
    "sdk": "org.kde.Sdk",
    "command": "kosmindoormap",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--share=network"
    ],
    "modules": [
        {
            "name": "kirigami-addons",
            "config-opts": [ "-DBUILD_TESTING=OFF" ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/kirigami-addons/kirigami-addons-1.1.0.tar.xz",
                    "sha256": "8ef352499139616b3142a6d7a92ab23749c568403ece8d854f064a941d085536"
                }
            ],
            "cleanup": [
                "/lib/cmake"
            ]
        },
        {
            "name": "kpublictransport",
            "config-opts": [ "-DBUILD_TESTING=OFF" ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [ { "type": "git", "url": "https://invent.kde.org/libraries/kpublictransport.git", "branch": "release/24.05" } ],
            "cleanup": [ "/include", "/lib/cmake", "/mkspecs", "/etc/xdg/*categories", "/share/qlogging-categories5" ]
        },
        {
            "name": "kopeninghours",
            "config-opts": [ "-DBUILD_TESTING=OFF", "-DBUILD_WITH_QT6=ON" ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [ { "type": "git", "url": "https://invent.kde.org/libraries/kopeninghours.git", "branch": "release/24.05" } ],
            "cleanup": [ "/include", "/lib/cmake", "/mkspecs", "/etc/xdg/*categories", "/share/qlogging-categories5" ]
        },
        {
            "name": "kosmindoormap",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "config-opts": [ "-DBUILD_TESTING=OFF", "-DBUILD_STANDALONE_APP=ON" ],
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ],
            "cleanup": [ "/include", "/lib/cmake", "/mkspecs", "/etc/xdg/*categories", "/share/qlogging-categories5" ]
        }
    ]
}
